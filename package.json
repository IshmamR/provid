{
  "name": "provid",
  "description": "ProVid from Promethewz",
  "author": "IshmamR",
  "main": "build/server/index.js",
  "private": false,
  "engines": {
    "node": "14.x.x"
  },
  "scripts": {
    "dev": "nodemon",
    "clean": "rm -rf .next && rm -rf ./build && rm -f ./tsconfig.tsbuildinfo",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "yarn clean && yarn run build:next && yarn run build:server",
    "start": "NODE_ENV=production node build/index.js -p $PORT",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.18.5",
    "axios": "^0.25.0",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-breakpoints": "^11.0.4",
    "styled-components": "^5.3.3",
    "youtube-dl": "^3.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "17.0.12",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@types/youtube-dl": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "next-plugin-antd-less": "^1.8.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  },
  "cacheDirectories": [".next/cache"]
}
